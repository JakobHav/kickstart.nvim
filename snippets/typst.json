	"Typst Table": {
		"prefix": "typsttable",
		"body": [
			"#table(",
			"  columns: 4,",
			"  inset: 5pt,",
			"  align: horizon,",
			"  table.header([], [], [], []),",
			"  [], [], [], [],",
			")"
		]
	},
	"Typst 2 Split": {
		"prefix": "typst3",
		"body": [
			"#let left = 50%",
			"#table(",
			"  columns: (left, 100% - left),",
			"  stroke: 0pt,",
			"  [",
			"",
			"  ],",
			"  [",
			"",
			"  ]",
			")"
		]
	},
	"Typst 3 Split": {
		"prefix": "typst2",
		"body": [
			"#let left = 33.3%",
			"#let right = 33.3%",
			"#table(",
			"  columns: (left, 100% - left - right, right),",
			"  stroke: 0pt,",
			"  [",
			"",
			"  ],",
			"  [",
			"",
			"  ],",
			"  [",
			"",
			"  ]",
			")"
		]
	}
