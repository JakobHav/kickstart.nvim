{
	"invoice-malibu": {
		"prefix": "invoice-malibu",
		"body": [
			"#import \"@local/invoice-sound:0.1.0\": conf",
			"#show: doc => conf(",
			"  doc,",
			"  empfänger: text[",
			"    Fly with me to Malibu\\",
			"    co. Jürgen Wieching \\",
			"    Hügelstraße 67 \\",
			"    49088 Osnabrück",
			"  ],",
			"  datum: \"${CURRENT_DATE}.${CURRENT_MONTH}.${CURRENT_YEAR}\",",
			"  jobs: (",
			"    (\"${CURRENT_DATE}.${CURRENT_MONTH}.${CURRENT_YEAR} Tontechnik \\\"Fly with me to Malibu\\\",  \", 250.00),",
			"  ),",
			")",
			"Sehr geehrter Herr Wieching,",
			"",
			"für die von Ihnen beauftragten tontechnischen Dienstleistungen im Rahmen eines Auftritts der Band \\\"Fly with me to Malibu\\\" stelle ich vereinbarungsgemäß folgene Beiträge in Rechnung:",
			""
		],
		"description": "Insert Invoice header"
	},
	"invoice": {
		"prefix": "invoice",
		"body": [
			"#import \"@local/invoice-sound:0.1.0\": conf",
			"#show: doc => conf(",
			"  doc,",
			"  empfänger: text[",
			"    ${1:name} \\",
			"    ${2:straße} \\",
			"    ${3:ort}",
			"  ],",
			"  datum: \"${CURRENT_DATE}.${CURRENT_MONTH}.${CURRENT_YEAR}\",",
			"  jobs: (",
			"    (\"${CURRENT_DATE}.${CURRENT_MONTH}.${CURRENT_YEAR} Tontechnik \\\"\\\",  \", ${0:250}.00),",
			"  ),",
			")",
			"Sehr geehrter Herr ${1:name},",
			"",
			"für die von Ihnen beauftragten tontechnischen Dienstleistungen im Rahmen eines Auftritts der Band \\\"\\\" im stelle ich vereinbarungsgemäß folgene Beiträge in Rechnung:",
			""
		],
		"description": "Insert Invoice header"
	},
